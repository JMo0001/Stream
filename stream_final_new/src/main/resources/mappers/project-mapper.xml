<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="project">
  
  <select id="selectList" resultType="ProjectVo">
	SELECT *
	FROM PROJECT
  </select>
  
  <insert id="insertList" parameterType="ProjectVo">
  	<!-- INSERT INTO PROJECT (PNO, USERID, PNAME, PCOMENT, PACCESS, PDATE, PSTARTDATE, PENDDATE)
	VALUES (#{pno}, #{userId}, #{pname}, #{pcoment}, #{paccess}, systimestamp, #{pstartDate}, #{pendDate}); -->
 	
	INSERT INTO PROJECT (PNO, USERID, PNAME, PCOMENT, PACCESS, PSTARTDATE, PENDDATE)
	VALUES (project_sequence.NEXTVAL, #{userId}, #{pname}, #{pcoment}, #{paccess}, #{pstartDate}, #{pendDate})
  </insert>
  
</mapper>
